<%= form_for prop_image, authenticity_token: true do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.hidden_field :image_cache %>
  </div><br><br>

  <div>
    <%= f.label :body %>
    <%= f.text_area :body %>
  </div><br><br>

  <div>
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div><br><br>

  <%= f.submit %>
<% end %>
